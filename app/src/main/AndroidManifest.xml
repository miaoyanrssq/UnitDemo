<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zjrb.bizman.unitdemo">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:name="com.zjrb.bizman.unitdemo.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light">
        <activity android:name="com.zjrb.bizman.unitdemo.ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.zjrb.bizman.unitdemo.ui.activity.TestActivity"></activity>
    </application>

</manifest>